<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.xyz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoficial"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menuusuario" />
        <activity android:name=".menuagente" />
        <activity android:name=".contacto" />
        <activity android:name=".registro" />
        <activity android:name=".busqueda" />
        <activity android:name=".oferta" />
        <activity android:name=".tipo" />
        <activity android:name=".detalle" />
        <activity android:name=".precio"></activity>
    </application>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>
</manifest>